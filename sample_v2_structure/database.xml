<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- version = the supported WoT version -->
<!-- onlineFolder = the name of the WoT online download folder -->
<!-- databaseVersion = the version of the database structure -->
<modInfoAlpha.xml version="1.4.0.2" onlineFolder="1.4.0" documentVersion="1.1">
  <!-- list each type of package list to go it's own xml file, rather than having one giant xml file -->
  <!-- globalDependencies and dependencies are static, have a list of categories -->
  <!-- the current setup is to have all the xml files be in the root zip file, or if beta db, in one folder (root repo) -->
  <!-- that way no folder paths are needed -->
  <globalDependencies file="globalDependencies.xml"/>
  <dependencies file="dependencies.xml"/>
  <categories>
    <category file="xvmPrebuiltConfigs.xml"/>
    <!-- <category file="xvmConfigBuilder.xml"/> -->
  </categories>
</modInfoAlpha.xml>