    <Packages>
      <Package PackageName="Tank_Skins_Misc_Camo_CamoSelector" Enabled="False" InstallGroup="4" PatchGroup="4" Name="Camouflages Selector by Polyacov_Yury" Type="single_dropdown1" Visible="True">
        <Size>43139</Size>
