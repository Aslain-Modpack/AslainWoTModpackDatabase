    <Package PackageName="GarageMods_LittleHelperCollection" Enabled="True" Tags="Patch,Script,ConfigurationFileJson" InstallGroup="4" PatchGroup="4" UID="ksbf9er3h535v1vd" Name="Little Helper Collection [by {author}] (READ DESCRIPTION)" Type="multi" Visible="True">
      <Size>126685</Size>
