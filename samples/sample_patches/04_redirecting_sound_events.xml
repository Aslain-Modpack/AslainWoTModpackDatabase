<patchs>
<!--Redirecting/remapping sound events using audio_mods.xml -->
<!--This takes two steps -->
<!-- 1: add the old event name -->
<patch>
  <type>xml</type>
  <mode>add</mode>
  <patchPath>app</patchPath>
  <file>\res_mods\versiondir\audioww\audio_mods.xml</file>
  <path>//audio_mods.xml/events</path>
  <search></search>
  <replace>event/name/vo_ally_killed_by_player</replace>
</patch>
<!-- 2: add the new event name -->
<patch>
  <type>xml</type>
  <mode>add</mode>
  <patchPath>app</patchPath>
  <file>\res_mods\versiondir\audioww\audio_mods.xml</file>
  <path>/audio_mods.xml/events/event[name = &quot;vo_ally_killed_by_player&quot;]</path>
  <search></search>
  <replace>mod/simple_sounds_teamkill</replace>
</patch>

