<Window x:Class="RelhaxDescriptionDesigner.Designs.Willster419.FirstWpfWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RelhaxDescriptionDesigner.Designs.Willster419"
        mc:Ignorable="d"
        Title="FirstWpfWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="This is a sample Wpf &quot;raw&quot; design. You can do just about anything here" VerticalAlignment="Top"/>
            <TextBox IsReadOnly="True" Text="Like this textbox."/>
            <Button Content="Why not a button?" Width="110" HorizontalAlignment="Left"/>
        </StackPanel>
    </Grid>
</Window>
